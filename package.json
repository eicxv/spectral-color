{
  "name": "spectral-color",
  "version": "0.1.0",
  "description": "Library for working with color and spectral distributions.",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/spectral-color.umd.js",
  "module": "./dist/spectral-color.es.js",
  "exports": {
    ".": {
      "import": "./dist/spectral-color.es.js",
      "require": "./dist/spectral-color.umd.js"
    }
  },
  "engines": {
    "node": "> 14"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eicxv/spectral-color.git"
  },
  "keywords": [
    "color",
    "color-matching-functions",
    "spectral-distribution",
    "spectrum",
    "illuminant",
    "cie"
  ],
  "author": "Einar Persson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eicxv/spectral-color/issues"
  },
  "homepage": "https://github.com/eicxv/spectral-color#readme",
  "devDependencies": {
    "@ts-docs/default-docs-structure": "^0.4.3",
    "@ts-docs/ts-docs": "^0.4.3",
    "@types/eslint": "^8.4.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "c8": "^7.11.2",
    "eslint": "^8.14.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "jest-matcher-deep-close-to": "^3.0.2",
    "prettier": "^2.6.2",
    "prettier-eslint": "^14.0.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-dts": "^1.1.1",
    "vitest": "^0.12.0"
  }
}
